<header id="home" class="jumbotron d-flex justify-content-center align-items-start">
    <div class="container  justify-content-center ">
        <div class="row justify-content-center">
            <h1 class="intro-text ">Hi, I'm <span> Mousab ALhamada</span></h1>
        </div>

        <div class="row justify-content-center">
            <h1 class="intro-description">I am a full-stack developer </h1>
        </div>
    </div>
    <% if(messages){%>
    <div class="toast" role="alert" aria-live="assertive" aria-atomic="true">
        <div class="toast-header">
            <i class="fa fa-bug" aria-hidden="true"></i>
            <strong class="mr-auto">Authintication eror</strong>
            <small class="text-muted">eror </small>
            <button type="button" class="ml-2 mb-1 close" data-dismiss="toast" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="toast-body">
            <%=messages%>
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $('.toast').toast('show');
        });
    </script>

    <%}%>

  

</header>